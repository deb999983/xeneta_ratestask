    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug_file",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "DJANGO_SETTINGS_MODULE": "api.settings",
                "MY_DB_USER": "postgres",
                "MY_DB_PASSWORD": "ratestask",
                "MY_DB_HOST": "ratestask_db",
                "MY_DB_PORT": "5432",
                "MY_DB_NAME": "postgres"
            },            
        },
        {
            "name": "debug_webserver",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "DJANGO_SETTINGS_MODULE": "api.settings",
                "MY_DB_USER": "postgres",
                "MY_DB_PASSWORD": "ratestask",
                "MY_DB_HOST": "ratestask_db",
                "MY_DB_PORT": "5432",
                "MY_DB_NAME": "postgres"
            }, 
            "args": [
                "runserver",
            ],
            "django": true,
            "justMyCode": false
        },        
    ]
}